<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<StackPane fx:id="rootPane" stylesheets="@/dark-theme.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mr.rpa.assistant.ui.listtask.BotMarketListController">
   <children>
      <AnchorPane fx:id="mainContainer" prefHeight="668" prefWidth="1000" maxHeight="668" style="-fx-border-color: -fx-primary; -fx-background-color: -fx-primary;">
         <children>
            <AnchorPane prefHeight="668.0" prefWidth="1000.0" styleClass="custom-pane" stylesheets="@/dark-theme.css">
               <children>
                  <TableView fx:id="tableView" layoutX="14.0" layoutY="14.0" prefWidth="10000.0"
                             AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                             AnchorPane.topAnchor="15.0">
                     <columns>
                        <TableColumn fx:id="seqCol" text="BOT编号"/>
                        <TableColumn fx:id="botNameCol" prefWidth="75.0" text="BOT名称"/>
                        <TableColumn fx:id="mainBotCol" prefWidth="75.0" text="BOT上级"/>
                        <TableColumn fx:id="despCol" prefWidth="75.0" maxWidth="200.0" text="描述" visible="false"/>
                        <TableColumn fx:id="versionCol" text="最新版本"/>
                        <TableColumn fx:id="downloadUrlCol" text="下载链接" visible="false"/>
                        <TableColumn fx:id="createdByCol" text="创建人"/>
                        <TableColumn fx:id="createdTimeCol" text="创建时间"/>
                        <TableColumn fx:id="operatingCol" text="操作"/>

                     </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                     </columnResizePolicy>
                  </TableView>
               </children>
            </AnchorPane>
         </children>
      </AnchorPane>
      </children>
</StackPane>
